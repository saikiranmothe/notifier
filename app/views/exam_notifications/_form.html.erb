<%= simple_form_for(@exam_notification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :last_date %>
    <%= f.input :exam_date %>
    <%= f.input :age_limit %>
    <%= f.input :company_name %>
    <%= f.input :edu_qualification %>
    <%= f.hidden_field :user_id, :value => current_user.id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
