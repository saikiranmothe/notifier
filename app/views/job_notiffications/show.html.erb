<%- model_class = JobNotiffication -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:company_name) %>:</strong></dt>
  <dd><%= @job_notiffication.company_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:vacancies) %>:</strong></dt>
  <dd><%= @job_notiffication.vacancies %></dd>
  <dt><strong><%= model_class.human_attribute_name(:last_date) %>:</strong></dt>
  <dd><%= @job_notiffication.last_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:exam_date) %>:</strong></dt>
  <dd><%= @job_notiffication.exam_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:age_limit) %>:</strong></dt>
  <dd><%= @job_notiffication.age_limit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:edu_qualifications) %>:</strong></dt>
  <dd><%= @job_notiffication.edu_qualifications %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @job_notiffication.user_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              job_notiffications_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_job_notiffication_path(@job_notiffication), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              job_notiffication_path(@job_notiffication),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
