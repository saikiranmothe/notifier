<%- model_class = JobNotiffication -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:company_name) %></th>
      <th><%= model_class.human_attribute_name(:vacancies) %></th>
      <th><%= model_class.human_attribute_name(:last_date) %></th>
      <th><%= model_class.human_attribute_name(:exam_date) %></th>
      <th><%= model_class.human_attribute_name(:age_limit) %></th>
      <th><%= model_class.human_attribute_name(:edu_qualifications) %></th>
      <th><% model_class.human_attribute_name(:user_id) %></th>
      <th><% model_class.human_attribute_name(:created_at) %></th>
      <th><% t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @job_notiffications.each do |job_notiffication| %>
      <tr>
        <td><%= link_to job_notiffication.id, job_notiffication_path(job_notiffication) %></td>
        <td><%= job_notiffication.company_name %></td>
        <td><%= job_notiffication.vacancies %></td>
        <td><%= job_notiffication.last_date %></td>
        <td><%= job_notiffication.exam_date %></td>
        <td><%= job_notiffication.age_limit %></td>
        <td><%= job_notiffication.edu_qualifications %></td>
        <td><% job_notiffication.user_id %></td>
        <td><%l job_notiffication.created_at %></td>

        <td>
          <% link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_job_notiffication_path(job_notiffication), :class => 'btn btn-mini' %>
          <% link_to t('.destroy', :default => t("helpers.links.destroy")),
                       job_notiffication_path(job_notiffication),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
        
      <div class="fb-share-button pull-right" data-href="http://www.google.com" data-type="button_count"></div>

      
      </tr>

    <% end %>
  </tbody>
</table>


<hr>
<%= link_to t('.new', :default => t("helpers.links.new")),
            new_job_notiffication_path,
            :class => 'btn btn-primary' %>


<div id="fb-root"></div>
